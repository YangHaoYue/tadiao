		    "style" :                                                                                    
		    {
		        "navigationBarTitleText": "注册",
		        "enablePullDownRefresh": false
		    }
		    
		}
        ,{
            "path" : "pages/register/agreement",
            "style" :                                                                                    
            {
                "navigationBarTitleText": "用户协议",
